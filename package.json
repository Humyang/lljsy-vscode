{
  "name": "vscode",
  "publisher": "humyang",
  "displayName": "vscode",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "activationEvents": [
    "onView:character"
  ],
  "contributes": {
    "commands": [
      {
        "command": "user.login",
        "title": "登录"
      },
      {
        "command": "character.pick",
        "title": "选择角色"
      },
      {
        "command": "lljsy.discover",
        "title": "探索"
      }
    ],
    "views": {
      "lljsy": [
        {
          "id": "character",
          "name": "角色"
        },
        {
          "id": "userAnimal",
          "name": "宠物"
        },
        {
          "id": "theWilds",
          "name": "野外"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "lljsy",
          "title": "XXXX",
          "icon": "media/dep.svg"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "character.helloWorld",
          "when": "view == character",
          "group": "navigation"
        },
        {
          "command": "lljsy.discover",
          "when": "view == theWilds",
          "group": "navigation"
        }
      ],
      "view/item/context": []
    }
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "compile": "webpack --mode none",
    "watch": "webpack --mode none --watch",
    "test-compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^10.12.21",
    "eslint": "^5.13.0",
    "ts-loader": "^6.0.2",
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.5",
    "fs-extra": "^8.0.1"
  },
  "dependencies": {
    "axios": "^0.19.0"
  }
}
